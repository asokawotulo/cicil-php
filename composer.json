{
    "name": "asokawotulo/cicil-php",
    "description": "PHP client for Cicil.co.id API",
    "version": "1.3.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Asoka Wotulo",
            "email": "asokawotulo2@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2.0",
        "guzzlehttp/guzzle": ">=6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.13",
        "vlucas/phpdotenv": "^5.3",
        "symfony/var-dumper": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Cicil\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cicil\\": [
                "tests/",
                "tests/Cicil/"
            ]
        }
    }
}
